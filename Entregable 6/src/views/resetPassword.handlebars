<div class="sidenav">
    <div class="login-main-text">
        <h2>Ecommerce</h2>
        <p>Recupera tu contraseña</p>
    </div>
</div>
<div class="main">
    <div class="col-md-6 col-sm-12">
        <div class="login-form">
            <form id="resetForm">
                <div class="form-group">
                    <label>Correo electronico</label>
                    <input name="email" type="email" class="form-control" placeholder="ejemplo123@ejemplo.com">
                </div>
                <div class="form-group">
                    <label>Nueva contraseña</label>
                    <input name="password" type="password" class="form-control" placeholder="xxxxxxxx">
                </div>
                <div class="form-group">
                    <label>Confirmar nueva contraseña</label>
                    <input type="password" class="form-control" placeholder="xxxxxxxx">
                </div>
                <button type="submit" class="btn btn-dark">Cambiar contraseña</button>
            </form>
            <a href="/register">Registrarse</a>
            <a href="/login">Iniciar sesión</a>
        </div>
    </div>
</div>

<script src="//code.jquery.com/jquery-1.11.1.min.js"></script>
<script src="//maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js"></script>
<script src="//cdn.jsdelivr.net/npm/sweetalert2@11"></script>
<script src="/js/resetPassword.js"></script>